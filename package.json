{
  "name": "aptamCORE",
  "productName": "aptamCORE",
  "version": "1.3.0",
  "main": "app.js",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=debug ./node_modules/.bin/vue-cli-service serve",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/vue-cli-service build",
    "lint": "vue-cli-service lint",
    "debug": "cross-env NODE_ENV=debug ELECTRON_ENABLE_LOGGING=true ./node_modules/.bin/electron .",
    "package:win": "electron-packager . aptamCORE --platform=win32 --arch=x64 --overwrite --ignore \"^/tools\" --ignore \"^/wiki\" --asar && mkdir aptamCORE-win32-x64\\tools && copy tools\\* aptamCORE-win32-x64\\tools && copy LICENSE aptamCORE-win32-x64\\LICENSE.aptamCORE",
    "clean": "rm -rf dist *-win32-x64",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "async": "^3.2.0",
    "aws-sdk": "^2.640.0",
    "chart.js": "^2.9.3",
    "cross-env": "^7.0.2",
    "d3": "^5.15.0",
    "d3.js": "^1.0.2",
    "electron-builder": "^22.4.1",
    "electron-log": "^4.1.0",
    "electron-packager": "^14.2.1",
    "electron-preferences": "^2.0.0",
    "electron-store": "^5.1.1",
    "handlebars": "^4.7.3",
    "jquery": "^3.4.1",
    "log4js": "^6.1.2",
    "mailgun-js": "^0.22.0",
    "peer": "^0.3.2",
    "serialize-javascript": "^3.0.0",
    "splitpanes": "^2.2.1",
    "sqlite3": "^4.2.0",
    "tabulator-tables": "^4.5.3",
    "temp": "^0.9.1",
    "venn.js": "^0.2.20",
    "vue": "^2.6.11",
    "vue-chartjs": "^3.5.0",
    "vue-cli": "^2.9.6",
    "vue-loading-overlay": "^3.3.2",
    "vue-multipane": "^0.9.5",
    "vue-split-pane": "0.0.8",
    "vue-split-panel": "^1.0.4",
    "vue-splitpane": "^1.0.6",
    "vue-splitpane-v2": "^2.0.1",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "ajv": "^6.12.0",
    "babel-eslint": "^10.1.0",
    "bootstrap-vue": "^2.7.0",
    "css-loader": "^3.4.2",
    "decompress-zip": "^0.2.2",
    "electron": "^8.1.0",
    "electron-build": "0.0.3",
    "electron-rebuild": "^1.11.0",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "node-gyp": "^6.1.0",
    "style-loader": "^1.1.3",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11",
    "vue-virtual-scroller": "^0.12.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
